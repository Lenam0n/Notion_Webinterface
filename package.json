{
  "name": "bewerbungspipeline",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/*/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel --filter api --filter frontend dev",
    "build": "pnpm -r build",
    "build:core": "pnpm -r --filter @shared/errors --filter @shared/types --filter @shared/utils --filter @notion/adapters --filter @tools/ps-orchestrator build",
    "build:apps": "pnpm -r --filter api --filter frontend build",
    "start": "pnpm -r --parallel --filter api --filter frontend start",
    "clean": "pnpm -r clean",
    "clean:dist": "pnpm -r exec rimraf dist",
    "clean:all": "pnpm -r exec rimraf dist node_modules",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
